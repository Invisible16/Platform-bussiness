  "scripts": {
    "prestart": "yarn install",
    "prebuild": "yarn install",
    "eslint": "eslint server/. desktop/. --config .eslintrc.json",
    "prettier": "prettier --find-config-path --write server/**/*.js desktop/**/*.js *.js *.json",
    "export-mysqldb": "mysqldump -u root -p goodflash > ./migration/db.sql",
    "import-mysqldb": "mysql -u root -p goodflash < ./migration/db.sql",
    "start-server": "NODE_ENV=development nodemon --inspect=7007 --watch server server/",
    "gulp-watch-desktop": "gulp --gulpfile ./desktop/gulpfile.js",
    "gulp-build-desktop": "gulp build --gulpfile ./desktop/gulpfile.js",
    "next-build-desktop": "next build ./desktop",
    "build": "run-p gulp-build-desktop next-build-desktop",
    "start": "run-p gulp-watch-desktop start-server"
  },
